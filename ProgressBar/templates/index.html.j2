<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Insert Title</title>
        <link rel="stylesheet" type="text/css" href="static/style.css"/>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script src="static/progress.js"></script>
        <script>
        $(document).ready(function(){
            $("#hide").click(function(){
                $("#bar").hide();
            });
            $(".show").click(function(){
                $("#bar").show();
            });
            $(".show2").click(function(){
                $("#bar2").show();
            });
        });
        </script>
    </head>
    <body>
        
             
    
    <div id="bar" class="counter" style="display:none">0</div>
    <div id="bar2" class="progressbar" style="display:none">
        <span class="progress"></span>
    </div>
    <button id="proceed" onclick="this.onclick=null; showText();" style="display:none">Proceed</button>
    <div id="text" style="display:none">
        <p>Progress bar complete!</p>
    </div>





















    <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
  Launch demo modal
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>













    </body>
</html>